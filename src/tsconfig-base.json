{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "target": "es5",
    "module": "commonjs",
    "rootDir": ".",
    "outDir": "../build",
    "moduleResolution": "node",
    "jsx": "react",
    "types": ["node", "express", "webpack-env"],
    "lib": ["dom", "es6", "es5"]
  }
}